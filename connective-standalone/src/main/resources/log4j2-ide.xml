<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout
				pattern="[%d{MM/dd/yyyy HH:mm:ss} %5level] [%c{1}%notEmpty{/%markerSimpleName}] - %highlight{%msg%n%throwable}" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="all">
			<AppenderRef ref="Console" level="debug" />
		</Root>
	</Loggers>
</Configuration>
